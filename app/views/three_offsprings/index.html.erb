<h1 class="alert alert-success text-center">Three Offsprings</h1>

<div class="text-center">
    <h3><%= link_to 'New Three offspring', new_three_offspring_path %></h3>
</div>
<p id="notice"><%= notice %></p>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Codecross</th>
            <th>Color</th>
            <th>Status</th>
            <th>Headsize</th>
            <th>Flowering</th>
            <th>Numpetals</th>
            <th>Form</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @three_offsprings.each do |three_offspring| %>
        <tr>
            <td><%= three_offspring.two_offspring.one_offspring.germination.seed.crossing.codeCross %>-<%=three_offspring.two_offspring.one_offspring.individual%></td>
            <td><%= three_offspring.color %></td>
            <td><%= three_offspring.status %></td>
            <td><%= three_offspring.headSize %></td>
            <td><%= three_offspring.flowering %></td>
            <td><%= three_offspring.numPetals %></td>
            <td><%= three_offspring.form %></td>
            <td><%= link_to 'Show', three_offspring %></td>
            <td><%= link_to 'Edit', edit_three_offspring_path(three_offspring) %></td>
            <td><%= link_to 'Destroy', three_offspring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<br>






