<p id="notice"><%= notice %></p>

<h1>Listing Two Offsprings</h1>

<table>
    <thead>
        <tr>
            <th>Codecross</th>
            <th>Individual</th>
            <th>Color</th>
            <th>Status</th>
            <th>Headsize</th>
            <th>Flowering</th>
            <th>Numpetals</th>
            <th>Form</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @two_offsprings.each do |two_offspring| %>
        <tr>
            <td><%= two_offspring.codeCross %></td>
            <td><%= two_offspring.individual %></td>
            <td><%= two_offspring.color ? two_offspring.color.colorName : 'N/A' %></td>
            <td><%= two_offspring.status %></td>
            <td><%= two_offspring.headSize %></td>
            <td><%= two_offspring.flowering %></td>
            <td><%= two_offspring.numPetals %></td>
            <td><%= two_offspring.form %></td>
            <td><%= link_to 'Show', two_offspring %></td>
            <td><%= link_to 'Edit', edit_two_offspring_path(two_offspring) %></td>
            <td><%= link_to 'Destroy', two_offspring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Two offspring', new_two_offspring_path %>
