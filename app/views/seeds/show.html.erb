<p id="notice"><%= notice %></p>
<div class="container">
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h2 class="text-left"><%= @seed.crossing.codeCross %></h2>
            </div>

            <div class="panel-body">
                <ul class="list-group">
                    <li class="list-group-item">CodeCross:
                        <span><strong><%= @seed.crossing.codeCross %></strong></span>
                    </li>

                    <li class="list-group-item">SowDate:
                        <span><strong><%= @seed.sowDate %></strong></span>
                    </li>

                    <li class="list-group-item">Origin:
                        <span><strong><%= @seed.origin %></strong></span>
                    </li>

                    <li class="list-group-item">NumSeeds:
                        <span><strong><%= @seed.numSeeds %></strong></span>
                    </li>

                    <li class="list-group-item">TotalWeight:
                        <span><strong><%= @seed.totalWeight %> </strong></span>
                    </li>

                    <li class="list-group-item">Week:
                        <span><strong><%= @seed.week %></strong></span>
                    </li>

                    <li class="list-group-item">Hydratation:
                        <span><strong><%= @seed.hydratation %></strong></span>
                    </li>

                    <li class="list-group-item">Status:
                        <span><strong><%= @seed.status %></strong></span>
                    </li>

                    <li class="list-group-item">DateOut:
                        <span><strong><%= @seed.dateOut %></strong></span>
                    </li>

                    <li class="list-group-item">Totalgermination:
                        <span><strong><%= @seed.totalGermination %></strong></span>
                    </li>

                    <li class="list-group-item">Percentagegermination:
                        <span><strong><%= @seed.percentageGermination %></strong></span>
                    </li>
                    
                    <li class="list-group-item">Germination:
                        <span><strong><%= @seed.germination %></strong></span>
                    </li>

                </ul>
            </div>

            <div class="panel-footer">
                <%= link_to 'Edit', edit_seed_path(@seed) %> |
                <%= link_to 'Back', seeds_path %>
            </div>
        </div>
    </div>
</div>





